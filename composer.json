{
    "name": "ceddyg/clara-parameter",
    "description": "Repository to create parameter from the database.",
    "keywords": ["laravel", "clara", "parameter"],
    "authors": [
        {
            "name": "CeddyG"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~6.0",
        "ceddyg/query-builder-repository": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "CeddyG\\ClaraParameter\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "CeddyG\\ClaraParameter\\ParameterServiceProvider"
            ],
            "aliases": {
                "ClaraParameter": "CeddyG\\ClaraParameter\\Facades\\ClaraParameter"
            }
        }
    }
}
